{
  "name": "vscode-js-ts-react-nextjs-snippets",
  "displayName": "JS/TS React/Next.js Snippets",
  "description": "Snippets for JS/TS, React/Next.js, React Query, SWR, Redux, Jest, React Hook Form, Emotion CSS, & many more",
  "icon": "images/logo.png",
  "version": "1.13.0",
  "publisher": "afifu",
  "keywords": [
    "vscode",
    "snippet",
    "javascript",
    "typescript",
    "react",
    "nextjs",
    "react-query",
    "react-hook-form",
    "zustand"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afiiif/vscode-js-ts-react-nextjs-snippets"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescript.json"
      }
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "npx ts-node ./src",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "typescript": "^4.9.4"
  }
}
