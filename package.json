{
  "name": "vscode-js-ts-react-nextjs-snippets",
  "displayName": "JS/TS React/Next.js Snippets",
  "description": "JS/TS, React/Next.js, React Query, & other Front-end snippets",
  "icon": "images/logo.png",
  "version": "1.0.0",
  "publisher": "afifu",
  "keywords": [
    "vscode",
    "snippet",
    "javascript",
    "typescript",
    "react",
    "nextjs",
    "react-query",
    "react-hook-form",
    "zustand"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afiiif/vscode-js-ts-react-nextjs-snippets"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescript.json"
      }
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "compile": "npx ts-node ./src",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
